<%= render 'shared/page_title', title: "ALL Names" %>
<ul class="nav nav-tabs">
  <li> <%= link_to "Temples", ptemples_path %> </li>
  <li class="active"> <%= link_to "Subscribers", customers_path %> </li>
  
</ul>



<div class="row">
	<div class="col-md-4 col-md-offset-4 center">
		<%= will_paginate %>
	</div>
</div>

<% @customers.each do |customer| %>
  <div class="row">
  	<div class="col-md-4 col-md-offset-4 center">
       <%= link_to gravatar_for(customer, size: 200), customer_path(customer) %>
       <p>
          <h5>Customer name: <%=customer.customername %></h5> 
       </p>  		

    </div>
  </div>
<% end %>